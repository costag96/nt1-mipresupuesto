@model TipoCuenta

@{
    ViewData["Title"] = "Borrar Tipo cuenta";
}

<h3 class="mb-3">¿Está seguro que desea borrar el tipo de cuenta @Model.Nombre?</h3>

<form asp-action="BorrarTipoCuenta">
    <input type="hidden" asp-for="Id"/>
    <button type="submit" class="btn btn-danger">Borrar</button>
    <button class="btn btn-secondary" asp-action="Index">Cancelar</button>
</form>